    "accumulate",
    "storage",
    "module",
    "modularity"
  ],
  "author": "Taras Panasiuk",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.4.1",
    "nyc": "^14.1.0",
    "sinon": "^7.3.2"
  },
  "ava": {
    "files": "**/*.test.js",
    "babel": false,
    "compileEnhancements": false
  },
  "nyc": {
    "include": "src",
